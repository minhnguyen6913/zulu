{extends file='jobby/master.tpl'}
{block name='css'}{/block}
{block name='script'}{/block}
{block name='body'}
    <!-- COMING SOON-->
    <section class="section section--h100 section--h100-nmobi" style="background: url('/assets/jobby/images/icon/bg-coming.jpg') center center / cover no-repeat;">
        <div class="section__content2">
            <div class="section-heading section-heading-2 m-b-50">
                <h2 class="section-heading__title">Coming Soon</h2>
                <div class="section-heading__line">
                    <img src="/assets/jobby/images/icon/line-white.png" alt="Line 2">
                </div>
            </div>
            <div class="au-countdown cd100 text-center">
                <div class="au-countdown__item white">
                    <div class="au-countdown__item-inner">
                        <span class="time days" data-months="1">1</span>
                        <span class="name">months</span>
                    </div>
                </div>
                <div class="au-countdown__item white">
                    <div class="au-countdown__item-inner">
                        <span class="time days" data-days="18">18</span>
                        <span class="name">days</span>
                    </div>
                </div>
                <div class="au-countdown__item white">
                    <div class="au-countdown__item-inner">
                        <span class="time hours" data-hours="21">21</span>
                        <span class="name">hours</span>
                    </div>
                </div>
                <div class="au-countdown__item white">
                    <div class="au-countdown__item-inner">
                        <span class="time minutes" data-minutes="5">05</span>
                        <span class="name">mins</span>
                    </div>
                </div>
                <div class="au-countdown__item white">
                    <div class="au-countdown__item-inner">
                        <span class="time seconds" data-seconds="36">36</span>
                        <span class="name">secs</span>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- END COMING SOON -->
{/block}
